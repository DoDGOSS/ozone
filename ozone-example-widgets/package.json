{
  "name": "@ozone/example-widgets",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server -d",
    "start:prod": "run-s build serve",
    "serve": "node ./bin/serve.js",

    "clean": "shx rm -rf build/*",
    "copy-owf-js": "shx cp ./public/owf-widget.js ./build",
    "check": "run-s format:check format lint",
    "format": "prettier --loglevel warn --write \"{src,tests}/**/*.{ts,tsx}\"",
    "format:check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint --project ./"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@blueprintjs/core": "3.15.1",
    "@blueprintjs/icons": "3.8.0",
    "core-js": "2.6.5",
    "formik": "1.5.2",
    "react": "16.8.6",
    "react-data-grid": "6.1.0",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",

    "@blueprintjs/table": "3.5.0",
    "@blueprintjs/timezone": "3.4.0",
    "ajv": "6.10.0",
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "core-js": "3.0.0",
    "formik": "1.5.1",
    "lodash": "4.17.11",
    "qs": "6.7.0",
    "react": "16.8.6",
    "react-confirm-alert": "^2.4.1",
    "react-dom": "16.8.6",
    "react-mosaic-component": "2.1.0",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-table": "6.9.2",
    "regenerator-runtime": "0.13.2",
    "rxjs": "6.4.0",
    "rxjs-hooks": "0.4.1",
    "space-lift": "0.8.7",
    "uuid": "3.3.2",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",

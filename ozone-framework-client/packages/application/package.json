{
  "name": "ozone-framework-client",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": "^10.10.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server -d",
    "build": "cross-env NODE_ENV=production webpack-cli -p",
    "build:test": "tsc -p tests/functional",
    "check": "run-s format:check format lint",
    "format": "prettier --loglevel warn --write \"{src,tests}/**/*.{ts,tsx}\"",
    "format:check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint --project ./src",
    "nightwatch": "nightwatch",
    "test": "jest --config ./jest-unit.config.js",
    "test:e2e": "jest --config ./jest-e2e.config.js --runInBand",
    "test:functional": "run-s build:test nightwatch",
    "test:clean": "jest --clearCache"
  },
  "dependencies": {
    "@blueprintjs/core": "3.12.0",
    "@blueprintjs/datetime": "3.7.0",
    "@blueprintjs/icons": "3.5.1",
    "@blueprintjs/select": "3.6.0",
    "@blueprintjs/table": "3.4.1",
    "@blueprintjs/timezone": "3.3.0",
    "@ozone/openapi-decorators": "^0.1.0",
    "ajv": "6.8.1",
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "core-js": "2.6.3",
    "formik": "1.4.3",
    "lodash": "4.17.11",
    "qs": "6.6.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-mosaic-component": "2.0.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-table": "6.9.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.4.0",
    "rxjs-hooks": "0.3.3",
    "space-lift": "0.8.7",
    "uuid": "3.3.2",
    "yup": "0.26.10"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/jest": "23.3.14",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.21",
    "@types/qs": "6.5.1",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.1",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "@types/react-table": "6.7.21",
    "@types/uuid": "3.4.4",
    "@types/webpack-env": "1.13.7",
    "@types/yup": "0.26.7",
    "awesome-typescript-loader": "5.2.1",
    "chromedriver": "2.46.0",
    "circular-dependency-plugin": "5.0.2",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "file-loader": "3.0.1",
    "geckodriver": "1.14.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "nightwatch": "1.0.18",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "react-hot-loader": "4.6.5",
    "sass-loader": "7.1.0",
    "script-loader": "0.7.2",
    "selenium-server": "3.141.59",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3",
    "url-loader": "1.1.2",
    "webpack": "4.29.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  }
}
